{
  "compilerOptions": {
    "experimentalDecorators": true,
    "baseUrl": "./",
    "noImplicitAny": false,
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react-jsx",
    "types": [
      "node",
      "react/next",
      "react-dom/next"
    ],
    "sourceMap": true
  },
  "include": [
    "src",
  ],
  "paths": {
    "*": ["interfaces/*.d.ts"]
  }
}
